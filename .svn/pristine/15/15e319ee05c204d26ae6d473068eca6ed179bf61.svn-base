<script src="../js/jquery.min.js"></script>
<script type="text/javascript">
    $(document).ready(function() {
        $(".select").width($(window).width()-117-22);
        $(".select dt").width($(window).width()-139-22);
        var selectWidth=$(".select").width();
        var wid = $(".finish-icon").width();
        var spanw = $(".wid > span").width();
        $(".input").width(selectWidth);
        $(".input1").width($(".input-list").width()-wid-spanw);
        $(".select dd").width(selectWidth);
        $(".select dd ul").width(selectWidth);
        $(".select dd li").width(selectWidth);
        $(".select").each(function(){
            var s=$(this);
            var z=parseInt(s.css("z-index"));
            var dt=$(this).children("dt");
            var dd=$(this).children("dd");
            var _show=function(){dd.slideDown(200);dt.addClass("cur");s.css("z-index",z+1);};   //展开效果
            var _hide=function(){dd.slideUp(200);dt.removeClass("cur");s.css("z-index",z);};    //关闭效果
            dt.click(function(){dd.is(":hidden")?_show():_hide();});
            dd.find("a").click(function(){dt.html($(this).html());_hide();});     //选择效果（如需要传值，可自定义参数，在此处返回对应的“value”值 ）
            $("body").click(function(i){ !$(i.target).parents(".select").first().is(s) ? _hide():"";});
        })
        $(".confirm").click(function () {
            window.location="self-judge.html"
        })
    });
</script>
