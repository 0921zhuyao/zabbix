*{
    margin: 0;
    padding: 0;
}
body{
    background: #F8F8FB;
}
.space{
    width: 100%;
    height: 5px;
    margin-top: 40px;
    background: #f8f8fb;
}
.list-title{
    width:100%;
    background: #fff;
}
.user-title{
    width:100%;
    height:190px;
    margin: 0 auto;
}
.user-img{
    width:100%;
    text-align: center;
    margin-top: 30px;
    margin-bottom: 10px;
}
.user-title p{
    text-align: center;
    color:#000;
    font-size:22px;
    display: inline-block;
    width:100%;
    margin: 20px auto 0;
    font-weight:bold;
}
.list-info{
    width:100%;
    height:80px;
    line-height: 80px;
    font-size:18px;
    font-weight:bold;
    /* padding-left: 40px; */
}
.list-info span{
    padding-left: 8%;
}
.middle{
    border-top:1px solid #d2d2d2;
    border-bottom:1px solid #d2d2d2;
}