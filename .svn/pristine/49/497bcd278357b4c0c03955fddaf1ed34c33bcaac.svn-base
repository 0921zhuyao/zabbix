body{
    background: #f8f8fb;
}
*{
    margin: 0;
    padding: 0;
}
#header{
    position: fixed;
    top: 0;
    left: 0;
    height: 45px;
    background: #333333;
    width: 100%;
    overflow: hidden;
}
.action-back{
    float: left;
}
.action-back img{
    width: 11px;
    margin: 14px 0 0 17px;
}
.mui-title {
    font-size: 16px;
    font-weight: 500;
    line-height: 44px;
    position: absolute;
    display: block;
    width: 70%;
    margin: 0 -10px;
    padding: 0;
    text-align: center;
    white-space: nowrap;
    color: #fff;
    right: 40px;
    left: 40px;
    display: inline-block;
    overflow: hidden;
    width: auto;
    margin: 0;
    text-overflow: ellipsis;
}
.submit-btn{
    float: right;
    height: 45px;
    line-height: 48px;
    margin-right: 18px;
    font-size: 15px;
    text-decoration: underline;
    color: #fff;
}
.space{
    width: 100%;
    height: 5px;
    margin-top: 45px;
    background: #f8f8fb;
}
.space1{
    width: 100%;
    height: 5px;
    background: #f8f8fb;
}
.list{
    list-style: none;
    width: 100%;
    background: #fff;
}
.list li{
    width: 100%;
    border-bottom: 1px solid #d2d2d2;
    height: 54px;
    line-height: 54px;
    overflow: hidden;
}
.list li:last-child{
    border-bottom:none;
}
.list li .list-icon{
    width: 17px;
    margin:19px 0 0 18px;
    float: left;
}
.list li span{
    float: left;
    margin-left: 10px;
    font-size: 16px;
}
.list li .arrow{
    float: right;
    width: 7px;
    margin: 23px 18px 0 13px;
}

.finish{
    background-color: #21a61a;
    color: #fff;
}
.no-finish{
    background-color: #ff2f2f;
    color: #fff;
}
.list li span {
    float: left;
    margin-left: 10px;
    font-size: 12px;
}
.list li .list-title>span{
    font-size: 16px;
}
.list li span:nth-child(2) {
    float: left;
    margin-left: 0;
}
.new {
    border: 1px solid #3061ec;
    color: #3061ec;
}
.list li{
    overflow: hidden;
    height: 100px;
}
.list-title{
    overflow: hidden;
    width: 100%;
    position: relative;
}
.list-msg{
    width: 100%;
    margin-top: -6px;
    overflow: hidden;
}
.msg-left{
    width: 80%;
}
p.list_address {
    width: 100%;
    color: #555;
    font-size: 12px;
    height: 20px;
    line-height: 20px;
    margin-left: 10px;
}
.list_de  span{
    color: #ababab;
    font-size: 12px;
    height: 20px;
    line-height: 20px;
}
.list li .list-title span{
    font-weight: bold;
}
.list li .list-title button {
    float: right;
    width: 100px;
    height: 100px;
    line-height:170px;
    font-size: 12px;
    position: absolute;
    right: -56px;
    top: -56px;
    transform:rotate(45deg) scale(0.9);
    -ms-transform:rotate(45deg) scale(0.9); 	/* IE 9 */
    -moz-transform:rotate(45deg) scale(0.9); 	/* Firefox */
    -webkit-transform:rotate(45deg) scale(0.9); /* Safari å’Œ Chrome */
    -o-transform:rotate(45deg) scale(0.9); 	/* Opera */
}
.list li .list-msg button{
    float: right;
    width: 16%;
    height: 26px;
    line-height: 26px;
    margin: -30px 20px 0 0;
    background: #fc4342;
    color: white;
    border-radius: 4px;
}